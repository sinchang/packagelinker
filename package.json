{
  "name": "packagelinker",
  "displayName": "packagelinker",
  "description": "quickly find the GitHub URL of the package",
  "version": "0.0.6",
  "publisher": "sinchang",
  "repository": "https://github.com/sinchang/packagelinker",
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "github",
    "PHP",
    "Python",
    "Ruby",
    "Node"
  ],
  "activationEvents": [
    "onCommand:extension.goToGitHub"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.goToGitHub",
        "title": "Go to GitHub"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "",
          "command": "extension.goToGitHub",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.60.0",
    "mocha": "^8.3.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "opn": "^6.0.0"
  }
}
